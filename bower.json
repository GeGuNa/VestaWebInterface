{
  "name": "vesta-web-interface",
  "homepage": "https://github.com/cdgco/VestaWebInterface",
  "authors": [
    "Carter Roeser <carter@cdgtech.one>"
  ],
  "description": "PHP interface for VestaCP, providing a beautiful UI, UX, features & integrations.",
  "main": "js/main.js",
  "keywords": [
    "vestacp",
    "cloudflare",
    "php",
    "interface",
    "control-panel",
    "php-interface",
    "admin-panel",
    "theme"
  ],
  "license": "GPL-3.0",
  "private": true,
  "ignore": [
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "animate.css": "latest",
    "bootstrap": "3",
    "bootstrap-datepicker": "uxsolutions/bootstrap-datepicker#latest",
    "bootstrap-colorpicker": "2.5.3",
    "bootstrap-select": "^1.13.1",
    "bootstrapvalidator": "latest",
    "font-awesome": "4.7.0",
    "footable": "latest",
    "hotkeysjs": "latest",
    "jquery": "2.2.4",
    "jquery-overlaps": "latest",
    "jquery-slimscroll": "latest",
    "metismenu": "latest",
    "moment": "latest",
    "phpmailer": "latest",
    "popper.js": "latest",
    "select2": "latest",
    "sweetalert2": "latest",
    "themify-icons": "latest",
    "waves": "latest"
  },
  "install": {
    "path": "plugins/components",
    "sources": {
      "animate.css": "bower_components/animate.css/animate.min.css",
      "bootstrap": {
        "mapping": [
          {
            "bower_components/bootstrap/dist/**": "dist"
          },
          {
            "bower_components/bootstrap/less/**": "less"
          }
        ]
      },
      "bootstrap-datepicker": [
        "bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker3.standalone.min.css",
        "bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"
      ],
      "bootstrap-colorpicker": "bower_components/bootstrap-colorpicker/dist/**",
      "bootstrap-select": "bower_components/bootstrap-select/dist/**",
      "font-awesome": {
        "mapping": [
          {
            "bower_components/font-awesome/css/**": "css"
          },
          {
            "bower_components/font-awesome/fonts/**": "fonts"
          }
        ]
      },
      "footable": [
        "bower_components/footable/compiled/footable.bootstrap.min.css",
        "bower_components/footable/compiled/footable.bootstrap.css",
        "bower_components/footable/compiled/footable.min.js"
      ],
      "hotkeysjs": "bower_components/hotkeysjs/dist/hotkeys.min.js",
      "jquery": "bower_components/jquery/dist/**",
      "jquery-overlaps": "bower_components/jquery-overlaps/jquery.overlaps.js",
      "metismenu": {
        "mapping": [
          {
            "bower_components/metismenu/dist/**": "dist"
          },
          {
            "bower_components/metismenu/src/**": "src"
          }
        ]
      },
      "moment": "bower_components/moment/min/moment.min.js",
      "phpmailer": "bower_components/phpmailer/**",
      "select2": [
        "bower_components/select2/dist/css/select2.min.css",
        "bower_components/select2/dist/js/select2.min.js"
      ],
      "sweetalert2": [
        "bower_components/sweetalert2/dist/sweetalert2.min.css",
        "bower_components/sweetalert2/dist/sweetalert2.min.js"
      ],
      "themify-icons": {
        "mapping": [
          {
            "bower_components/themify-icons/css/**": "css"
          },
          {
            "bower_components/themify-icons/fonts/**": "fonts"
          }
        ]
      }
    }
  }
}
